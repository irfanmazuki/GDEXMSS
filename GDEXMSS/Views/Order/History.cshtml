@model IEnumerable<GDEXMSS.Models.order>
@{
    ViewBag.Title = "Registration";
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">

<head>
    <!-- Meta Tag -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name='copyright' content=''>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Title Tag  -->
    <title>Eshop - eCommerce HTML5 Template.</title>
    <!-- Favicon -->
    <link rel="icon" type="../image/png" href="images/favicon.png">
    <!-- Web Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

    <!-- StyleSheet -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css" type="text/css" runat="server">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/css/magnific-popup.min.css" type="text/css" runat="server">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/lib/font-awesome/css/all.css" type="text/css" runat="server">
    <!-- Fancybox -->
    <link rel="stylesheet" href="~/Content/css/jquery.fancybox.min.css" type="text/css" runat="server">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="~/Content/css/themify-icons.css" type="text/css" runat="server">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="~/Content/css/niceselect.css" type="text/css" runat="server">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/Content/css/animate.css" type="text/css" runat="server">
    <!-- Flex Slider CSS -->
    <link rel="stylesheet" href="~/Content/css/flex-slider.min.css" type="text/css" runat="server">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Content/css/owl-carousel.css" type="text/css" runat="server">
    <!-- Slicknav -->
    <link rel="stylesheet" href="~/Content/css/slicknav.min.css" runat="server" type="text/css">
    <!--ADDED adminLTE-->
    <!-- <link rel="stylesheet" href="../css/adminlte.css"> -->
    <!-- Eshop StyleSheet -->
    <link rel="stylesheet" href="~/Content/css/reset.css" type="text/css" runat="server">
    <link rel="stylesheet" href="~/Content/css/responsive.css" type="text/css" runat="server">
    <link rel="stylesheet" href="~/Content/css/style.css" type="text/css" runat="server">
</head>

<body class="js">

    <!-- Preloader -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- End Preloader -->
    <!-- Header -->
    <header class="header shop">
        <!-- Topbar -->
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-5 col-md-12 col-12">
                        <!-- Top Left -->
                        <div class="top-left">
                            <ul class="list-main">
                                <li><i class="fa-solid fa-headphones-simple"></i> +60 9620 3006</li>
                                <li><i class="fa fa-solid fa-envelope"></i> support@gdexpress.com</li>
                            </ul>
                        </div>
                        <!--/ End Top Left -->
                    </div>
                    <div class="col-lg-7 col-md-12 col-12">
                        <!-- Top Right -->
                        <div class="right-content">
                            <ul class="list-main">
                                <li><i class="fa fa-solid fa-map-pin"></i> Branch location</li>
                                <li><i class="fa fa-solid fa-star"></i> <a href="#">Daily deal</a></li>
                                <li><i class="fa fa-solid fa-user"></i> <a href="#">My account</a></li>
                                <li><i class="fa fa-solid fa-power-off"></i>@Html.ActionLink("Login", "Login")</li>
                            </ul>
                        </div>
                        <!-- End Top Right -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Topbar -->
        <div class="middle-inner">
            <div class="container">
                <div class="row">
                    <div class="col-lg-2 col-md-2 col-12">
                        <!-- Logo -->
                        <div class="logo">
                            <a href="index.html"><img src="https://seeklogo.com/images/G/gdex-logo-6838448C0F-seeklogo.com.png" alt="logo"></a>
                        </div>
                        <!--/ End Logo -->
                        <!-- Search Form -->
                        <div class="search-top">
                            <div class="top-search"><a href="#0"><i class="ti-search"></i></a></div>
                            <!-- Search Form -->
                            <div class="search-top">
                                <form class="search-form">
                                    <input type="text" placeholder="Search here..." name="search">
                                    <button value="search" type="submit"><i class="ti-search"></i></button>
                                </form>
                            </div>
                            <!--/ End Search Form -->
                        </div>
                        <!--/ End Search Form -->
                        <div class="mobile-nav"></div>
                    </div>
                    <div class="col-lg-8 col-md-7 col-12">
                        <div class="search-bar-top">
                            <div class="search-bar">
                                <select>
                                    <option selected="selected">All Category</option>
                                    <option>Limited Edition</option>
                                    <option>Clothes</option>
                                    <option>Stationery</option>
                                    <option>Miniature</option>
                                </select>
                                <form>
                                    <input name="search" placeholder="Search Products Here....." type="search">
                                    <button class="btnn"><i class="fas fa-solid fa-search"></i></button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-3 col-12">
                        <div class="right-bar">
                            <!-- Search Form -->
                            <div class="sinlge-bar">
                                <a href="#" class="single-icon"><i class="fa fa-solid fa-heart"></i></a>
                            </div>
                            <!--
                            <div class="sinlge-bar">
                                <a href="#" class="single-icon"><i class="fa fa-user-circle-o"></i></a>
                            </div>-->
                            <div class="sinlge-bar shopping">
                                <a href="#" class="single-icon"><i class="fa fa-solid fa-shopping-bag"></i></i> <span class="total-count">2</span></a>
                                <!-- Shopping Item -->
                                <div class="shopping-item">
                                    <div class="dropdown-cart-header">
                                        <span>2 Items</span>
                                        <a href="#">View Cart</a>
                                    </div>
                                    <ul class="shopping-list">
                                        <li>
                                            <a href="#" class="remove" title="Remove this item"><i class="fa fa solid fa-times"></i></a>
                                            <a class="cart-img" href="#"><img src="~/Content/images/sampulraya.jpg" alt="#"></a>
                                            <h4><a href="#">Sampul Raya 2021</a></h4>
                                            <p class="quantity">1x - <span class="amount">RM20.00</span></p>
                                        </li>
                                        <li>
                                            <a href="#" class="remove" title="Remove this item"><i class="fa fa solid fa-times"></i></a>
                                            <a class="cart-img" href="#"><img src="~/Content/images/shirt.png" alt="#"></a>
                                            <h4><a href="#">GDEX Limited Shirt (M)</a></h4>
                                            <p class="quantity">2x - <span class="amount">RM80.00</span></p>
                                        </li>
                                    </ul>
                                    <div class="bottom">
                                        <div class="total">
                                            <span>Total</span>
                                            <span class="total-amount">RM100.00</span>
                                        </div>
                                        <a href="checkout.html" class="btn animate">Checkout</a>
                                    </div>
                                </div>
                                <!--/ End Shopping Item -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="cat-nav-head">
                    <div class="row">
                        <div class="col-lg-9 col-12">
                            <div class="menu-area">
                                <!-- Main Menu -->
                                <nav class="navbar navbar-expand-lg">
                                    <div class="navbar-collapse">
                                        <div class="nav-inner">
                                            <ul class="nav main-menu menu navbar-nav">
                                                <li><a href="https://localhost:44319/products">Home</a></li>
                                                <li><a href="https://localhost:44319/User/Profile">Profile</a></li>
                                                <li class="active"><a href="https://localhost:44319/Order/History">Order History</a></li>
                                                <li>
                                                    <a href="#">eWallet<i class="fa fa-solid fa-chevron-down"></i></a>
                                                    <ul class="dropdown">
                                                        <li><a href="https://localhost:44319/Wallet/Balance">Balance</a></li>
                                                        <li><a href="https://localhost:44319/Wallet/Topup">Topup</a></li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </nav>
                                <!--/ End Main Menu -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Header Inner -->
    </header>
    <!--/ End Header -->
    <!-- Breadcrumbs -->
    <div class="breadcrumbs">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="bread-inner">
                        <ul class="bread-list">
                            <li><a href="index.html">Order<i class="fa fa-solid fa-arrow-right"></i></a></li>
                            <li class="active"><a href="new.html">History</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Breadcrumbs -->
    <section class="shop checkout section">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <ul class="navbar nav ml-auto">
                                <li class="mr-auto p-2">
                                    <h2 class="card-title">Order History</h2>
                                </li>
                                <li class="p-2">
                                    <div class="form-group form-group-sm" style="width: 210px;">
                                        <select name="state-province" id="state-province">
                                            <option selected="selected" disabled>Sort by</option>
                                            <option>Default</option>
                                            <option>ID (Ascending)</option>
                                            <option>ID (Descending)</option>
                                            <option>Delivering</option>
                                            <option>Cancelled</option>
                                            <option>Delivered</option>
                                        </select>
                                    </div>
                                </li>
                                <li class="p-2">
                                    <div class="input-group input-group-sm" style="width: 150px;">
                                        <input type="text" name="table_search" class="form-control" placeholder="Search">
                                        <div class="input-group-append">
                                            <button type="submit" class="btn btn-default">
                                                <i class="fas fa-search"></i>
                                            </button>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body table-responsive p-0">
                            <table class="table table-hover text-nowrap">
                                <thead>
                                    <tr>
                                        <th>@Html.DisplayNameFor(model => model.createdDT)</th>
                                        <th>@Html.DisplayNameFor(model => model.orderID)</th>
                                        <th>@Html.DisplayNameFor(model => model.userID)</th>
                                        <th>@Html.DisplayNameFor(model => model.amountPaid)</th>
                                        <th>@Html.DisplayNameFor(model => model.consignment)</th>
                                        <th>Items</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.createdDT)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.orderID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.userID)
                                        </td>
                                        <td>
                                            RM
                                            @Html.DisplayFor(modelItem => item.amountPaid)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.consignment)
                                        </td>
                                        <td></td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.status)
                                        </td>

                                    </tr>
                                }
                                </tbody>
                            </table>
                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->
                </div>
            </div>
        </div>
    </section>
    <!-- Start Footer Area -->
    <footer class="footer">
        <!-- Footer Top -->
        <div class="footer-top section">
            <div class="container">
                <div class="row">
                    <div class="col-lg-5 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer about">
                            <p class="text">Established in 1997, GD Express (GDEX) has grown steadily spanning over 150 stations in Malaysia and Singapore. GDEX is listed on the Main Board of Bursa Malaysia with 2 main operating segments, namely, Express Delivery and Logistics.</p>
                            <p class="call">Got Question? Call us 24/7<span><a href="tel:123456789">+0123 456 789</a></span></p>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer links">
                            <h4>Information</h4>
                            <ul>
                                <li><a href="#">About Us</a></li>
                                <li><a href="#">Faq</a></li>
                                <li><a href="#">Terms & Conditions</a></li>
                                <li><a href="#">Contact Us</a></li>
                                <li><a href="#">Help</a></li>
                            </ul>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer links">
                            <h4>Customer Service</h4>
                            <ul>
                                <li><a href="#">Payment Methods</a></li>
                                <li><a href="#">Money-back</a></li>
                                <li><a href="#">Returns</a></li>
                                <li><a href="#">Shipping</a></li>
                                <li><a href="#">Privacy Policy</a></li>
                            </ul>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-3 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer social">
                            <h4>Get In Tuch</h4>
                            <!-- Single Widget -->
                            <div class="contact">
                                <ul>
                                    <li>No 19 Jalan Tandang,</li>
                                    <li>46050 Petaling Jaya,</li>
                                    <li>Selangor Darul Ehsan Malaysia</li>
                                    <li>+03-77856818</li>
                                </ul>
                            </div>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Footer Top -->
        <div class="copyright">
            <div class="container">
                <div class="inner">
                    <div class="row">
                        <div class="col-lg-6 col-12">
                            <div class="left">
                                <p>Copyright © 2020 <a href="http://www.gdexpress.com" target="_blank">GD Express Merchandise Shop System</a> - All Rights Reserved.</p>
                            </div>
                        </div>
                        <div class="col-lg-6 col-12">
                            <div class="right">
                                <p>Download our app</p>
                                <div class="logo">
                                    <a href="index.html"><img src="http://assets.stickpng.com/images/5a902dbf7f96951c82922875.png" alt="logo" width="100" /></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- /End Footer Area -->
    <!-- Jquery -->
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/jquery-migrate-3.0.0.js"></script>
    <script src="~/Content/js/jquery-ui.min.js"></script>
    <!-- Popper JS -->
    <script src="~/Content/js/popper.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!-- Color JS -->
    <script src="~/Content/js/colors.js"></script>
    <!-- Slicknav JS -->
    <script src="~/Content/js/slicknav.min.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/Content/js/owl-carousel.js"></script>
    <!-- Magnific Popup JS -->
    <script src="~/Content/js/magnific-popup.js"></script>
    <!-- Waypoints JS -->
    <script src="~/Content/js/waypoints.min.js"></script>
    <!-- Countdown JS -->
    <script src="~/Content/js/finalcountdown.min.js"></script>
    <!-- Nice Select JS -->
    <script src="~/Content/js/nicesellect.js"></script>
    <!-- Flex Slider JS -->
    <script src="~/Content/js/flex-slider.js"></script>
    <!-- ScrollUp JS -->
    <script src="~/Content/js/scrollup.js"></script>
    <!-- Onepage Nav JS -->
    <script src="~/Content/js/onepage-nav.min.js"></script>
    <!-- Easing JS -->
    <script src="~/Content/js/easing.js"></script>
    <!-- Active JS -->
    <script src="~/Content/js/active.js"></script>
</body>

</html>


